{
  "name": "w4services/w4_cookiebar",
  "type": "typo3-cms-extension",
  "version": "1.0.1",
  "description": "W4 extension to provide a GDPR (DE: 'DSGVO') cookie banner.",
  "homepage": "https://www.w-4.com",
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/w4holding/w4_cookiebar.git"
      }
  ],
  "authors": [{
      "name": "W4 Services GmbH",
      "email": "info@w-4.com",
      "role": "Developer"
  }],
  "license": [
      "GPL-2.0-or-later"
  ],
  "config": {
      "sort-packages": true
  },
  "require": {
      "typo3/cms-core": "^11.5"
  },
  "support": {
      "issues": "https://github.com/w4holding/w4_cookiebar/issues"
  },
  "replace": {
      "typo3-ter/w4_cookiebar": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "cms-package-dir": "{$vendor-dir}/typo3/cms",
          "web-dir": ".Build/public",
          "extension-key": "w4_cookiebar"
      }
  }
}
